import random
print("奥头世界极致拉郎机")
num_cp = int(input("输入作案人数："))
final_cplist = []
while num_cp > 0:
    index = random.randrange(0, len(at_list))
    final_cplist.append(at_list[index])
    num_cp -= 1
string = '×'.join(final_cplist)
print("!!!您的拉郎产品是!!!")
print("٩(ˊᗜˋ*)و "+string+" ٩(ˊᗜˋ*)و")
